package org.bpmnwithactiviti.chapter10.rules

import org.bpmnwithactiviti.chapter10.model.LoanApplicant;
import org.bpmnwithactiviti.chapter10.model.CreditCheckResult;

rule "CreditCheckRule"
    
	when
		la: LoanApplicant(income > (2 * loanAmount))
		res : CreditCheckResult()
	then
		res.setCreditCheckPassed(true);

end